<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.phoebewong.osilayerapp">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Layer1" />
        <activity android:name=".Layer2" />
        <activity android:name=".Layer3" />
        <activity android:name=".Layer4" />
        <activity android:name=".Layer5" />
        <activity android:name=".Layer6" />
        <activity android:name=".Layer7" />
        <activity android:name=".Quiz"></activity>
    </application>

</manifest>